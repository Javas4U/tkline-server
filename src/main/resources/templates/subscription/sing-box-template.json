{
    "log": {
        "level": "info",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "127.0.0.1",
            "listen_port": 7890
        }
    ],
    "outbounds": [
        ${OUTBOUNDS},
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "final": "${FINAL_OUTBOUND}"
    },
    "__comment": "以下定义用于模板渲染，渲染后将被移除",
    "{{ define \"vless\" }}": {
        "tag": "${NODE_NAME}",
        "type": "vless",
        "server": "${SERVER_IP}",
        "server_port": ${SERVER_PORT
        },
        "uuid": "${UUID}",
        "flow": "xtls-rprx-vision",
        "tls": {
            "enabled": true,
            "server_name": "www.cloudflare.com",
            "utls": {
                "enabled": true,
                "fingerprint": "chrome"
            },
            "reality": {
                "enabled": true,
                "public_key": "YOUR_REALITY_PUBLIC_KEY",
                "short_id": "a1b2c3d4"
            }
        }
    },
    "{{ define \"hy2\" }}": {
        "tag": "${NODE_NAME}",
        "type": "hysteria2",
        "server": "${SERVER_IP}",
        "server_port": ${SERVER_PORT
        },
        "password": "${PASSWORD}",
        "obfs": {
            "type": "salamander",
            "password": "NTdhMjdhMjAwMjRkYWEzYg=="
        },
        "tls": {
            "enabled": true,
            "server_name": "${SNI}",
            "alpn": [
                "h3"
            ]
        }
    }
}